{
  "name": "functions",
  "scripts": {
    "build": "babel src --extensions \".ts\" --out-dir lib",
    "build-test": "babel test --extensions \".ts\" --out-dir lib-test",
    "serve": "SET NODE_ENV=development& npm run build && firebase emulators:start --only functions",
    "debug": "npm run build && firebase emulators:start --inspect-functions --only functions,auth",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions,auth",
    "logs": "firebase functions:log",
    "test": "SET NODE_ENV=development& mocha --recursive \"./lib-test/*.js\"",
    "test2": "mocha --reporter dot --exit --timeout 3000",
    "test3": "SET NODE_ENV=development& npm run build && firebase emulators:exec --only functions,auth \"npm run test2\""
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@google-cloud/logging": "^9.8.0",
    "@google-cloud/storage": "^5.8.3",
    "@types/busboy": "^1.3.0",
    "@types/express-session": "^1.17.4",
    "@types/formidable": "^1.2.1",
    "@types/node": "^14.14.37",
    "@types/nodemailer": "^6.4.1",
    "@types/shortid": "0.0.29",
    "axios": "^0.26.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "body-parser": "^1.20.0",
    "busboy": "^1.5.0",
    "chai": "^4.3.6",
    "class-validator": "^0.13.2",
    "connect-busboy": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "firebase": "^9.6.8",
    "firebase-admin": "^10.0.2",
    "firebase-backend": "^0.1.5",
    "firebase-functions": "^3.20.0",
    "firebase-tools": "^10.6.0",
    "formidable": "^1.2.2",
    "http-status-codes": "^2.2.0",
    "mocha": "^9.2.2",
    "moment": "^2.29.1",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.8",
    "mysql": "^2.18.1",
    "node-fetch": "^3.2.3",
    "nodemailer": "^6.5.0",
    "query-string": "^7.1.1",
    "sanitize-filename": "^1.6.3",
    "shortid": "^2.2.16",
    "sinon": "^13.0.1",
    "supertest": "^6.1.6",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "xhr2": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-transform-regenerator": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "firebase-functions-test": "^0.3.2",
    "typescript": "4.5.5"
  },
  "private": true
}
